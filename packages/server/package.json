{
  "name": "server",
  "version": "0.0.0",
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.13.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.0"
  },
  "scripts": {
    "clean": "rm -rf lib && node copyConfig.js",
    "lint": "eslint \"src/**/*\"",
    "build": "npm run clean && tsc",
    "dev": "npm run clean && tsc -w",
    "test": "firebase emulators:exec 'jest --runInBand'",
    "test:coverage": "firebase emulators:exec 'jest --runInBand --coverage --watchAll'",
    "test:subcutaneous": "npm run clean && jest --runInBand --testPathPattern=tests/subcutaneous",
    "test:watch": "firebase emulators:exec 'jest --runInBand --watchAll'",
    "test:watch:dev": "run-p dev test:watch",
    "view:coverage": "live-server coverage/lcov-report"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.1.7",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.38",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.20",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "axios": "^0.21.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "faker": "^5.1.0",
    "firebase-functions-test": "^0.2.0",
    "firebase-tools": "^9.1.0",
    "jest": "^26.6.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "recursive-copy": "^2.0.11",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
